{
  "name": "minesminis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:server:dev\" \"vite\"",
    "build": "tsc -b && vite build && npm run server:install",
    "start": "cd server && NODE_ENV=production node server.js",
    "preview:prod": "vite preview --host 0.0.0.0 --port 5000",
    "lint": "eslint .",
    "preview": "vite preview",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm run start",
    "server:install": "cd server && npm install"
  },
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@supabase/supabase-js": "^2.76.1",
    "bad-words": "^4.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.2",
    "gsap": "^3.13.0",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.548.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.23.1",
    "recharts": "^3.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
